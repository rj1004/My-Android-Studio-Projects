<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rahulcompany.whatsappstatussaver">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_icons8_whatsapp"
        android:label="Whatsapp Status Saver"
        android:roundIcon="@drawable/ic_icons8_whatsapp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".VideoViewActivity"/>
        <activity android:name=".ImageViewActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-5556989253157684~3898515978"/>
    </application>

</manifest>